<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
	<TargetFramework>net6.0-windows</TargetFramework>
	<UseWindowsForms>true</UseWindowsForms>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="res\folder.png" />
    <None Remove="res\images\colours\blue.png" />
    <None Remove="res\images\colours\green.png" />
    <None Remove="res\images\colours\orange.png" />
    <None Remove="res\images\colours\purple.png" />
    <None Remove="res\images\colours\red.png" />
    <None Remove="res\images\colours\yellow.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="res\folder.png" />
    <Resource Include="res\images\colours\blue.png" />
    <Resource Include="res\images\colours\green.png" />
    <Resource Include="res\images\colours\orange.png" />
    <Resource Include="res\images\colours\purple.png" />
    <Resource Include="res\images\colours\red.png" />
    <Resource Include="res\images\colours\yellow.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resource1.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource1.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resource1.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource1.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
